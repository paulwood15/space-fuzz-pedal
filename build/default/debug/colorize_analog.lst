                      00126         BANKSEL DST
                      00127         SUBWF   DST,F         ; Subtract DST(low) - SRC(low)
                      00128         BANKSEL SRC
                      00129         MOVF    SRC+1,W       ; Now get high byte of subtrahend
                      00130         BTFSS   STATUS,C        ; If there was a borrow, rather than
                      00131         INCF    SRC+1,W       ; decrement high byte of dst we inc src
                      00132         BANKSEL DST
                      00133         SUBWF   DST+1,F       ; Subtract the high byte and we're done
