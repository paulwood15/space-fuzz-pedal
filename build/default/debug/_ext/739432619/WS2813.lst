                      00113 ;
                      00114 ; 16 bit unsigned subtraction with carry out.
                      00115 ; Word format is little endian (LSB at lower address)
                      00116 ; Operation is DST = DST - SRC
                      00117 ;
                      00118 ; (This from the "tips and tricks" seminar handout)
                      00119 ;
                      00120 ; DST is replaced, SRC is preserved, Carry is set correctly
                      00121 ;
                      00122 ;
                      00123 SUB16   MACRO   DST, SRC
                      00124         BANKSEL SRC
                      00125         MOVF    SRC,W         ; Get low byte of subtrahend
                      00126         BANKSEL DST
                      00127         SUBWF   DST,F         ; Subtract DST(low) - SRC(low)
                      00128         BANKSEL SRC
                      00129         MOVF    SRC+1,W       ; Now get high byte of subtrahend
                      00130         BTFSS   STATUS,C        ; If there was a borrow, rather than
                      00131         INCF    SRC+1,W       ; decrement high byte of dst we inc src
                      00132         BANKSEL DST
                      00133         SUBWF   DST+1,F       ; Subtract the high byte and we're done
                      00134         ENDM
