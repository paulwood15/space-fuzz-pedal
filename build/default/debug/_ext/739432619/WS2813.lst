                      00063 ;
                      00064 ; 16 bit unsigned compare, returns Z and C set appropriately
                      00065 ; Compares the registers "CMP_A" and "CMP_B"
                      00066 ; Flag states and what they mean:
                      00067 ;
                      00068 ;       Z true - X & Y are equal
                      00069 ;       C true - X > Y
                      00070 ;       C false - X < Y
                      00071 ;        
                      00072 CMP16   MACRO   X, Y
                      00073         MOV16   X, _REG_A
                      00074         SUB16   _REG_A,Y
                      00075         MOVF    _REG_A,W
                      00076         IORWF   _REG_A+1,W
