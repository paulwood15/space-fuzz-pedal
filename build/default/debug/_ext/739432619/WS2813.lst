                      00181 ;
                      00182 ; Increment 16 bit value, sets Z on exit.
                      00183 ;
                      00184 ; Operation: DST++
                      00185 ;
                      00186 INC16   MACRO   DST        
                      00187         INCFSZ  (DST),W         ; Add one to low byte
                      00188         DECF    (DST)+1,F       ; No carry (negates next step)
                      00189         INCF    (DST)+1,F       ; Add one to high byte
                      00190         MOVWF   (DST)           ; Store updated low byte back.
                      00191         IORWF   (DST)+1,W       ; Set Z flag
                      00192         ENDM
