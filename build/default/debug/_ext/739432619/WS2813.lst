                      00142 ;
                      00143 ; 16 bit unsigned addition with carry out.
                      00144 ; Operation: DST = DST + SRC                       
                      00145 ;
                      00146 ; DST is replaced, SRC is preserved, Carry is set correctly
                      00147 ;
                      00148 ADD16   MACRO   DST,SRC 
                      00149         MOVF    (SRC),W         ; Get low byte
                      00150         ADDWF   (DST),F         ; Add to destination
                      00151         MOVF    (SRC)+1,W       ; Get high byte
                      00152         BTFSC   STATUS,C        ; Check for carry
                      00153         INCF    (SRC)+1,W       ; Add one for carry
                      00154         ADDWF   (DST)+1,F       ; Add high byte into DST
                      00155         ENDM
                      00156 
                      00157 ;
                      00158 ; 16 bit Add Immediate
                      00159 ; Operation: DST = DST + Constant
                      00160 ;
                      00161 ; DST is updated, carry is set correctly.
                      00162 ;
                      00163 ADDI16  MACRO   DST,AD
                      00164         MOVLW   LOW (AD)
                      00165         ADDWF   DST,F
                      00166         MOVLW   HIGH (AD)
                      00167         BTFSC   STATUS,C
                      00168         MOVLW   (HIGH (AD)) + 1
                      00169         ADDWF   (DST)+1,F
                      00170         ENDM
                      00171 ;
                      00172 ; Negate 16 bit value
                      00173 ; Find two's complement value of a 16 bit number
                      00174 ;
                      00175 NEG16   MACRO   DST
                      00176         COMF    (DST)
                      00177         COMF    (DST)+1
                      00178         INC16   DST
                      00179         ENDM
