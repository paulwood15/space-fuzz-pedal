                      00133         MOVLW   LOW (CONST)
                      00134         SUBWF   (DST), F
                      00135         MOVLW   HIGH (CONST)
                      00136         BTFSS   STATUS, C
                      00137         MOVLW   (HIGH (CONST))+1
                      00138         SUBWF   (DST)+1,F
                      00139         ENDM
